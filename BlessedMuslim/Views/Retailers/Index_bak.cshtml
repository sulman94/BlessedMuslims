@model IEnumerable<BlessedMuslim.Models.Retailers>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_LayoutRetailers.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.BusinessName)
